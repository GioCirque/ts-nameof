{
  "name": "ts-nameof",
  "version": "0.10.3",
  "description": "nameof in TypeScript",
  "main": "dist/main.js",
  "typings": "ts-nameof.d.ts",
  "scripts": {
    "test": "npm run copy-test-files && nyc --reporter=lcov mocha --opts mocha.opts",
    "test-ci": "npm run test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "rimraf dist && tsc",
    "copy-test-files": "rimraf temp && copyfiles -u 2 \"./src/tests/testFiles/**/*{.js,.txt}\" \"./temp\"",
    "lint": "tslint src/**/*.ts",
    "do-publish": "npm run lint && npm run test && npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsherret/ts-nameof.git"
  },
  "keywords": [
    "typescript",
    "code"
  ],
  "author": "David Sherret",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dsherret/ts-nameof/issues"
  },
  "homepage": "https://github.com/dsherret/ts-nameof#readme",
  "typescript": {
    "definition": "ts-nameof.d.ts"
  },
  "dependencies": {
    "es6-promise": "^3.2.1",
    "glob": "^7.0.6",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.5",
    "@types/mocha": "^5.2.0",
    "@types/node": "^10.0.4",
    "@types/through2": "^2.0.33",
    "copyfiles": "^2.0.0",
    "coveralls": "^2.11.6",
    "gulp": "^3.9.1",
    "mocha": "^2.3.3",
    "nyc": "^11.7.1",
    "rimraf": "^2.6.2",
    "ts-node": "^6.0.2",
    "tslint": "^3.13.0",
    "typescript": "^2.8.3"
  }
}
